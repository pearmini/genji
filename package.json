{
  "name": "genji-notebook",
  "version": "0.0.3",
  "description": "Interactive JavaScript Notebook.",
  "bin": "bin/genji.js",
  "scripts": {
    "test": "jest",
    "lint": "prettier --write --ignore-unknown .",
    "ci": "run-s test lint",
    "start": "cd demo && nodemon ../bin/genji.js dev",
    "build": "cd demo && ../bin/genji.js build",
    "deploy": "cd demo && ../bin/genji.js deploy",
    "prepare": "husky install"
  },
  "files": [
    "public",
    "lib",
    "assets",
    "bin",
    "command"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pearmini/genji-notebook.git"
  },
  "author": "https://github.com/pearmini",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pearmini/genji-notebook/issues"
  },
  "homepage": "https://github.com/pearmini/genji-notebook#readme",
  "dependencies": {
    "commander": "^9.0.0",
    "file-system": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "eslint": "^8.9.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
